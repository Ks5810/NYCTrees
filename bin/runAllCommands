#!/bin/bash

EXE=NYCTreeInfoExe
IN_FILE=inputfiles/street_trees_full.csv
nl='
'
touch temp.txt

#create a temp file for storing commands

#loop files in commandfiles
for file in commandfiles/*; do
    cat $file >> temp.txt
  done

echo $nl
echo "running these comands:$nl"
cat temp.txt

#execute the program
echo "reading inputfile... $nl"
$EXE $IN_FILE temp.txt

rm temp.txt

